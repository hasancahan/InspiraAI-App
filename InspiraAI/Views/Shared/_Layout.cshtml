<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - InspiraAI</title>

    <link href="~/css/footer.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com" rel="preconnect">
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin="anonymous">
    <link href="~/css/inspira.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">

</head>

<body>
    <div class="navbar-container">
        <a href="/" aria-current="page" class="navbar2_logo-link w-nav-brand w--current" aria-label="home">
            <img src="~/content/image/ins_logo.png" style="width:180px;" />
        </a>
        <nav role="navigation" id="w-node-_46a1a930-a937-37fe-bcca-d83df445cdc2-f445cdbd" class="navbar2_menu is-page-height-tablet w-nav-menu">
            <a href="/Inspira/Dashboard" aria-current="page" class="navbar2_link w-nav-link w--current">Dashboard</a>
            <a href="/Ai/DiscoverArtworks" class="navbar2_link w-nav-link">Discover Arts</a>
            <a href="https://www.hasancahan.com/" target="_blank" class="navbar2_link w-nav-link">Connect</a>
        </nav>
        <div id="w-node-_46a1a930-a937-37fe-bcca-d83df445cdc9-f445cdbd" class="navbar2_button-wrapper">
            <a href="/Inspira/Account" class="profile-link w-inline-block">
                <img src="https://assets.website-files.com/6427381726a472874096a798/64273cba2a7f285dc9430b86_profile%20image.png" loading="lazy" alt="" class="profile-link-image">
            </a>
            <a href="/Ai/GenerateArtworks" class="button is-navbar2-button gradient-background w-button">Generate Art ✨</a>
            <div class="navbar2_menu-button w-nav-button" style="-webkit-user-select: text;" aria-label="menu" role="button" tabindex="0" aria-controls="w-nav-overlay-0" aria-haspopup="menu" aria-expanded="false">
                <div class="menu-icon2">
                    <div class="menu-icon2_line-top"></div>
                    <div class="menu-icon2_line-middle">
                        <div class="menu-icon_line-middle-inner"></div>
                    </div>
                    <div class="menu-icon2_line-bottom"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="container">

        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <div class="footer">
        <div id="button"></div>
        <div id="container">
            <div id="cont">
                <div class="footer_center">
                    <div>
                        <ul>
                            <a href="https://twitter.com/julesforrest">Twitter</a>
                            <a href="https://codepen.io/julesforrest">Codepen</a>
                            <a href="mailto:julesforrest@gmail.com">Email</a>
                            <a href="https://dribbble.com/julesforrest">Dribbble</a>
                            <a href="https://github.com/julesforrest">Github</a>

                            <p>👋</p>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/js/berai.026e66568.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

    <script src="js/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">
        WebFont.load({
            google: {
                families: ["Inter:300,regular,500,600"]
            }
        });
    </script>

    <script type="text/javascript">
        ! function (o, c) {
            var n = c.documentElement,
                t = " w-mod-";
            n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
        }(window, document);
    </script>
</body>
</html>
